varnishtest "Test null vmod"

server s1 {
       rxreq
       txresp
} -start

varnish v1 -vcl+backend {
	import null from "${vmod_topbuild}/src/.libs/libvmod_null.so";

	sub vcl_recv {
		error 418 "TEAPOT";
	}
	sub vcl_error {
		null.synth("TEST",4);
		return (deliver);
	}
} -start

client c1 {
	txreq -url "/"
	rxresp
	expect resp.bodylen == 4
}

client c1 -run
